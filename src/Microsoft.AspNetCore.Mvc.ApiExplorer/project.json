{
  "description": "ASP.NET Core MVC API explorer functionality for discovering metadata such as the list of controllers and actions, and their URLs and allowed HTTP methods.",
  "version": "1.2.0-*",
  "packOptions": {
    "repository": {
      "type": "git",
      "url": "git://github.com/aspnet/mvc"
    },
    "tags": [
      "aspnetcore",
      "aspnetcoremvc"
    ]
  },
  "buildOptions": {
    "warningsAsErrors": true,
    "keyFile": "../../tools/Key.snk",
    "nowarn": [
      "CS1591"
    ],
    "xmlDoc": true
  },
  "dependencies": {
    "Microsoft.AspNetCore.Mvc.Core": {
      "target": "project"
    },
    "Microsoft.AspNetCore.Mvc.Formatters.Json": "1.2.0-*",
    "Microsoft.Extensions.ClosedGenericMatcher.Sources": {
      "version": "1.2.0-*",
      "type": "build"
    },
    "Microsoft.Extensions.PropertyHelper.Sources": {
      "version": "1.2.0-*",
      "type": "build"
    },
    "NETStandard.Library": "1.6.2-*"
  },
  "frameworks": {
    "net451": {},
    "netstandard1.6": {}
  }
}