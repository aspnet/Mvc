{
  "dependencies": {
    "dotnet-test-xunit": "1.0.0-rc3-000000-01",
    "Microsoft.AspNetCore.Mvc.Core": "1.0.3",
    "Microsoft.AspNetCore.Mvc.DataAnnotations": "1.0.3",
    "Microsoft.AspNetCore.Mvc.Formatters.Xml": "1.0.3",
    "Microsoft.AspNetCore.Mvc.Razor": "1.0.3",
    "Microsoft.AspNetCore.Mvc.Razor.Host": "1.0.3",
    "Microsoft.AspNetCore.Mvc.TestCommon": {
      "version": "1.0.0-*",
      "type": "build"
    },
    "Microsoft.AspNetCore.Mvc.ViewFeatures": "1.0.3",
    "Microsoft.AspNetCore.Testing": "1.0.1"
  },
  "testRunner": "xunit",
  "buildOptions": {
    "define": [
      "__RemoveThisBitTo__GENERATE_BASELINES"
    ],
    "keyFile": "../../tools/Key.snk",
    "embed": {
      "include": "TestFiles/**"
    },
    "compile": {
      "include": "**/*.cs",
      "exclude": [
        "TestFiles/**",
        "bin",
        "obj"
      ]
    }
  },
  "frameworks": {
    "netcoreapp1.0": {
      "imports": [
        "dnxcore50",
        "portable-net451+win8"
      ],
      "dependencies": {
        "Microsoft.NETCore.App": {
          "version": "1.0.3",
          "type": "platform"
        },
        "moq.netcore": "4.4.0-beta8",
        "System.Diagnostics.Process": "4.1.0",
        "System.Diagnostics.TraceSource": "4.0.0"
      }
    },
    "net451": {
      "dependencies": {
        "Moq": "4.2.1312.1622"
      }
    }
  }
}